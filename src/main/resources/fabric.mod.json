{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",

  "name": "${name}",
  "description": "This mod allows the ver1.19.3 creative inventory layout to be used with versions lower than ver1.19.3.",
  "authors": [
    "Plastoid501"
  ],
  "contact": {
    "homepage": "https://github.com/Plastoid501/",
    "sources": "https://github.com/Plastoid501/new-creative-inventory"
  },

  "icon": "assets/new-creative-inventory/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.plastoid501.nci.NewCreativeInventory"
    ]
  },
  "mixins": [
    "new-creative-inventory.mixins.json"
  ],

  "depends": {
    "minecraft": "${minecraft_dependency}"
  }
}
